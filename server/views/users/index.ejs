<h2 class="center">Users</h2>
<table class="table">
    <thead>
    <tr>
        <th>Name</th>
        <th>Age</th>
    </tr>
    </thead>
    <tbody>
    <% users.forEach(user => { %>
    <tr>
        <td>
            <a href="<%=`/users/${user._id}`%>">
                <%= user.name %>
            </a>
        </td>
        <td>
            <%= user.age %>
        </td>
        <td>
            <a href="<%=`/users/${user._id}?_method=DELETE` %>" onclick="return confirm('Are you sure you want to delete this record?')" a>Delete</a>
        </td>
    </tr>
    <% }); %>
    </tbody>
</table>
